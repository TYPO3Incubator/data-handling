{
	"name": "typo3/cms-data-handling",
	"type": "typo3-cms-framework",
	"description": "TYPO3 Data Handling",
	"homepage": "https://typo3.org",
	"license": ["GPL-2.0+"],
	"minimum-stability": "dev",
	"require": {
		"php" : "^7.0",
		"dbellettini/eventstore-client": "dev-master"
	},
	"replace": {
		"data-handling": "*"
	},
	"bin": [
		"bin/typo3"
	],
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\DataHandling\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"TYPO3\\CMS\\DataHandling\\Tests\\": "Tests/"
		}
	}
}
