{
	"name": "typo3/cms-datahandling",
	"type": "typo3-cms-framework",
	"description": "TYPO3 Data Handling",
	"homepage": "https://typo3.org",
	"license": ["GPL-2.0+"],

	"require": {
		"php" : "^7.0"
	},
	"replace": {
		"core": "*"
	},
	"bin": [
		"bin/typo3"
	],
	"extra": {
		"typo3/cms": {
			"Package": {
				"protected": true,
				"partOfFactoryDefault": true,
				"partOfMinimalUsableSystem": true
			}
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\DataHandling\\": "Classes/"
		},
	},
	"autoload-dev": {
		"psr-4": {
			"TYPO3\\CMS\\DataHandling\\Tests\\": "Tests/"
		}
	}
}
